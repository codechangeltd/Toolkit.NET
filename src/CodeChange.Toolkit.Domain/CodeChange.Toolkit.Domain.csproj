<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	<TargetFramework>net7.0</TargetFramework>
	<Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>Toolkit.NET</Product>
    <Description>A library containing domain interfaces, base classes and tools for Domain Driven Design (DDD) projects.</Description>
    <Copyright>Copyright © Code Change Ltd 2023</Copyright>
    <Authors>Code Change Ltd</Authors>
    <AssemblyVersion>5.4.0</AssemblyVersion>
    <Version>5.4.0</Version>
    <licenseUrl>https://github.com/codechangeltd/Toolkit.NET/blob/master/LICENSE</licenseUrl>
    <PackageProjectUrl>https://github.com/codechangeltd/Toolkit.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/codechangeltd/Toolkit.NET</RepositoryUrl>
    <PackageIcon>Icon.png</PackageIcon>
    <RequireLicenseAcceptance>false</RequireLicenseAcceptance>
	<PackageReleaseNotes>Made all existing ValueObjects cloneable.</PackageReleaseNotes>
    <Tags>CodeChange Toolkit Domain DDD .NET DOTNET C#</Tags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="BinaryFog.NameParser" Version="3.2.3" />
    <PackageReference Include="CSharpFunctionalExtensions" Version="2.38.1" />
    <PackageReference Include="MediatR" Version="12.0.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\CodeChange.Toolkit\CodeChange.Toolkit.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Icon.png" Pack="true" PackagePath="" />
  </ItemGroup>
</Project>
