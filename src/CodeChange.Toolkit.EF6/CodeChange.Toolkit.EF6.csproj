<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>Toolkit.NET</Product>
    <Description>A library containing domain interfaces, base classes and tools for Entity Framework 6 implementations.</Description>
    <Copyright>Copyright Â© Code Change Ltd 2020</Copyright>
    <Authors>Code Change Ltd</Authors>
    <AssemblyVersion>3.0.0</AssemblyVersion>
    <Version>3.0.0-preview1</Version>
    <licenseUrl>https://github.com/codechangeltd/Toolkit.NET/blob/master/LICENSE</licenseUrl>
    <PackageProjectUrl>https://github.com/codechangeltd/Toolkit.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/codechangeltd/Toolkit.NET</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/codechangeltd/Toolkit.NET/master/src/CodeChange.Toolkit.Domain/Icon.png</PackageIconUrl>
    <RequireLicenseAcceptance>false</RequireLicenseAcceptance>
    <PackageReleaseNotes>Converted project to .NET Standard 2.1</PackageReleaseNotes>
    <Tags>CodeChange Toolkit Domain EF6 EntityFramework .NET DOTNET C#</Tags>
    <LangVersion>7.1</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="EntityFramework" Version="6.4.0" />
